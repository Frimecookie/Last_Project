<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.project.mapper.VoiceMapper">
  
  <!-- 음성 등록 -->
  <insert id="insertVoiceFile" parameterType="kr.project.entity.Voice">
    INSERT INTO voice_files (VOICE-NUM, USER_ID, FILE_PATH)
    VALUES (#{USER_ID}, #{filePath})
  </insert>
  
  <!-- 음성 목록 -->
  <select id="voiceList" parameterType="java.lang.String" resultType="kr.project.entity.voice">
		select * 
		from voices 
		where USER_ID = #{USER_ID}
  </select>
</mapper>